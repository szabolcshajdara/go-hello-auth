{
  "name": "Go + gcloud (Cloud Run)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "googlecloudtools.cloudcode"
      ]
    }
  },
  "postCreateCommand": "go version && gcloud version"
}
